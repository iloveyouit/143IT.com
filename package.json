{
  "name": "143it-website",
  "version": "1.1.0",
  "description": "Modern MSP website with Next.js 14, MDX blog, Framer Motion animations, and Docker deployment. Automate. Evolve. Dominate.",
  "author": "143IT",
  "private": true,
  "keywords": [
    "MSP",
    "Managed Services",
    "IT Automation",
    "Cloud Infrastructure",
    "DevOps",
    "AI Integration",
    "Next.js",
    "TypeScript",
    "TailwindCSS",
    "MDX"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t 143it-website:latest .",
    "docker:run": "docker run -d --name 143it-web -p 3000:3000 143it-website:latest",
    "docker:stop": "docker stop 143it-web && docker rm 143it-web",
    "docker:logs": "docker logs -f 143it-web",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f web"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.6",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.292.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-slug": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
